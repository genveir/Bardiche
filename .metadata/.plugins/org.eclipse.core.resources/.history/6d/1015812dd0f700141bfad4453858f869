package com.geertenvink.Bardiche;

import com.stephengware.java.glaive.GlaivePlan;
import com.stephengware.java.planware.AxiomTree;

public class BardichePlan extends GlaivePlan {
	public final String protagonist;
	public final BardicheProblem problem;
	
	public BardichePlan(BardichePlan plan) {
		super(plan);
		this.problem = problem;
		this.protagonist = plan.protagonist;
	}
	
	public BardichePlan(String name, BardicheProblem problem, AxiomTree axiomTree) {
		super(name, problem, axiomTree);
		this.problem = problem;
		this.protagonist = problem.protagonist;
	}
}
