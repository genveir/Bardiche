package com.geertenvink.Bardiche;

import com.stephengware.java.glaive.GlaivePlan;
import com.stephengware.java.glaive.GlaiveSearchNode;

public class BardicheSearchNode extends GlaiveSearchNode {
	public BardicheSearchNode(GlaivePlan plan) {
		super(plan);
	}
	
	public boolean isEvaluated() {
		try {
			getRelaxedPlan();
		} catch (IllegalStateException e) {
			System.out.println("nope"); //SCRIPTIE
			return false;
		}
		return true;
	}
}
