package com.geertenvink.Bardiche;

import com.stephengware.java.glaive.GlaiveRelaxedPlanBuilder;
import com.stephengware.java.glaive.GlaiveSearchNode;
import com.stephengware.java.planware.ig.IntentionGraph;
import com.stephengware.java.planware.logic.Expression;

public class BardicheRelaxedPlanBuilder extends GlaiveRelaxedPlanBuilder {

	public BardicheRelaxedPlanBuilder(IntentionGraph planGraph, Expression goal) {
		super(planGraph, goal);
	}

	@Override
	protected final void evaluate(GlaiveSearchNode node) {
		super.evaluate(node);
	}
}
