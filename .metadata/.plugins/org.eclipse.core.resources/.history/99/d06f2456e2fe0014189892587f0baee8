package com.geertenvink.Bardiche;

import com.stephengware.java.planware.ArgumentMap;
import com.stephengware.java.planware.logic.Expression;

public class BardicheGoal {
	private final Expression goal;
	
	public BardicheGoal(ArgumentMap arguments) {
		this.goal = arguments.get(Bardiche.PROBLEM).goal;
	}
	
	public Expression getGoal() {
		return goal;
	}
}
